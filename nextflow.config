manifest {
    mainScript = 'distiller.nf'
}

docker {
    runOptions = '-u $(id -u):$(id -g)'
}

timeline {
    enabled = true
}

trace {
    enabled = true
}

executor {
    cpus = 40
}

params {
    fastqc_cpus = 4
    map_cpus = 8
    merge_cpus = 8
    parse_cpus = 8
    filter_make_pairs_cpus = 8
    bin_cpus = 20
}
